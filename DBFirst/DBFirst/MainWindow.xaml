<Window x:Class="DBFirst.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBFirst"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid DataContext="{Binding}">
        <TextBox x:Name="Id" HorizontalAlignment="Left" Height="23" Margin="191,43,0,0" TextWrapping="Wrap" Text="{Binding Id}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Franchise" HorizontalAlignment="Left" Height="23" Margin="191,102,0,0" TextWrapping="Wrap" Text="{Binding Franchise1}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Owner" HorizontalAlignment="Left" Height="23" Margin="191,164,0,0" TextWrapping="Wrap" Text="{Binding Owner}" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="218,234,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="Id" HorizontalAlignment="Left" Margin="103,43,0,0" VerticalAlignment="Top"/>
        <Label Content="Franchise" HorizontalAlignment="Left" Margin="103,99,0,0" VerticalAlignment="Top"/>
        <Label Content="Owner" HorizontalAlignment="Left" Margin="103,164,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
